{
  "name": "discord-bots",
  "private": true,
  "author": "limitlesspc",
  "repository": {
    "type": "git",
    "url": "https://github.com/limitlesspc/discord-bots.git"
  },
  "scripts": {
    "setup": "pnpm i && cd thor && pnpm i && cd ../thor-music && pnpm i && cd .. && pnpm run gen",
    "setup-thor": "pnpm i && cd thor && pnpm i && cd .. && pnpm run gen",
    "setup-thor-music": "cd thor-music && pnpm i && cd .. && pnpm run gen && pnpm thor-music",
    "update": "NPM_CHECK_INSTALLER=pnpm npm-check -u && cd thor && npm-check -u && cd ../thor-music && npm-check -u && cd ..",
    "build": "pnpm run gen",
    "gen": "prisma generate",
    "studio": "prisma studio",
    "thor": "node -r @swc-node/register thor/src/main.ts",
    "thor-music": "node -r @swc-node/register thor-music/src/main.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "discord.js": "13.8.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "prisma": "^3.15.2",
    "typescript": "4.7.4"
  }
}
