{
  "private": true,
  "author": "limitlesspc",
  "repository": {
    "type": "git",
    "url": "https://github.com/limitlesspc/discord-bots.git"
  },
  "scripts": {
    "setup": "pnpm i && cd thor && pnpm i && cd ../thor-music && pnpm i && cd ..",
    "update": "NPM_CHECK_INSTALLER=pnpm npm-check -u && cd thor && npm-check -u && cd ../thor-music && npm-check -u && cd ..",
    "gen": "prisma generate",
    "studio": "prisma studio",
    "deploy": "prisma migrate deploy",
    "thor": "node -r @swc-node/register thor/src/main.ts",
    "thor-music": "node -r @swc-node/register thor-music/src/main.ts",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@supabase/supabase-js": "^1.35.3",
    "discord.js": "13.8.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "prisma": "^3.15.2",
    "typescript": "4.7.4"
  }
}
