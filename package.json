  "repository": "github:limitlesspc/discord-bots",
  "scripts": {
    "setup": "pnpm i && cd thor && pnpm i && cd ../thor-music && pnpm i && cd ..",
    "update": "NPM_CHECK_INSTALLER=pnpm npm-check -u && cd thor && npm-check -u && cd ../thor-music && npm-check -u && cd ..",
    "start": "tmuxinator start",
    "thor": "cd thor && pnpm run start",
    "thor-music": "cd thor-music && pnpm run start",
    "lint": "eslint --ignore-path .gitignore .",
    "gen-cmd": "cd thor && pnpm run gen-cmd && cd .."
  },
  "dependencies": {
    "discord.js": "^13.6.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
