{
  "name": "discord-bots",
  "private": true,
  "scripts": {
    "commit": "./commit.sh",
    "update": "NPM_CHECK_INSTALLER=pnpm npm-check -u; pnpm run -r --sequential update",
    "lint": "xo",
    "dev:raya": "turbo run dev --filter raya-thor",
    "start:raya": "turbo run start --filter raya-thor",
    "dev:jay": "turbo run dev --filter jay",
    "start:jay": "turbo run start --filter jay",
    "deploy": "turbo run deploy"
  },
  "devDependencies": {
    "@in5net/prettier-config": "^1.1.0",
    "@in5net/xo-config": "^1.5.0",
    "husky": "^8.0.3",
    "turbo": "^1.10.14",
    "typescript": "5.2.2",
    "xo": "^0.56.0"
  }
}